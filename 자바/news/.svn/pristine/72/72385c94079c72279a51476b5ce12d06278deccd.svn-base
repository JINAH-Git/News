//네트워크 그래프 자료를 담기위한 VO(Value Object)클래스
package news.vo;

public class NetworkgraphVO 
{
	int nwgno;			//네트워크그래프관리번호
	int cdno;			//기사관리번호
	String noun1;		//명사1
	String noun2;		//명사2
	String nwgweight;	//가중치
	String rdrefine;	//정제차수
	
	public int getNwgno()	 	 {	return nwgno;		}
	public String getNoun1() 	 {	return noun1;		}
	public String getNoun2()	 {	return noun2;		}
	public String getNwgweight() {	return nwgweight;	}
	public int getCdno() 		 {	return cdno;		}
	public String getRdrefine()  {	return rdrefine;	}
	
	public void setNwgno(int nwgno)		       {  this.nwgno = nwgno;			}	
	public void setNoun1(String noun1) 	       {  this.noun1 = noun1;			}	
	public void setNoun2(String noun2) 		   {  this.noun2 = noun2;			}
	public void setNwgweight(String nwgweight) {  this.nwgweight = nwgweight;	}
	public void setCdno(int cdno) 			   {  this.cdno = cdno;				}
	public void setRdrefine(String rdrefine)   {  this.rdrefine = rdrefine;		}
	
}
